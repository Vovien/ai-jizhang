<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="are_talking_about">正在讨论</string>
    <string name="published_under_your_point_view">发表下您的观点…</string>
    <string name="suppose_you_want_to_see">猜你想看</string>
    <style name="CircleStyle">
		<item name="cornerFamily">rounded</item>
		<item name="cornerSize">8dp</item>
	</style>
    <declare-styleable name="FollowButton">
        <attr format="dimension" name="fb_radius"/>
        <attr format="dimension" name="fb_success_line_width"/>
        <attr format="dimension" name="fb_add_line_width"/>
        <attr format="color" name="fb_success_background"/>
        <attr format="color" name="fb_add_background"/>
        <attr format="color" name="fb_success_line_color"/>
        <attr format="color" name="fb_add_line_color"/>
        <attr format="dimension" name="fb_left_offset"/>
        <attr format="dimension" name="fb_top_offset"/>
        <attr format="dimension" name="fb_border_width"/>
        <attr format="color" name="fb_border_color"/>
    </declare-styleable>
    <declare-styleable name="TickSeekBar">
<!--        //seekBar-->
        <attr format="float" name="tsb_max"/><!-- the max value of seekBar to seek, default 100-->
        <attr format="float" name="tsb_min"/><!-- the min value of seekBar to seek, default 0 -->
        <attr format="float" name="tsb_progress"/><!-- the current progress value of seekBar, default 0-->
        <attr format="boolean" name="tsb_progress_value_float"/><!--set the value of seekBar to float type, default false-->
        <attr format="boolean" name="tsb_clear_default_padding"/><!-- set seekBar's leftPadding&rightPadding to zero, default false, default padding is 16dp-->
        <attr format="integer" name="tsb_ticks_count"/><!--seekBar's ticks count, default zero-->
        <attr format="boolean" name="tsb_seek_smoothly"/><!--true to seek smoothly when tick count >0, default false-->
        <attr format="boolean" name="tsb_r2l"/><!--compat make app local change,like arabic local> 2, default false-->
        <attr format="boolean" name="tsb_user_seekable"/><!--prevent user from seeking,only can be seek by setProgress(), default false-->
        <attr format="boolean" name="tsb_only_thumb_draggable"/><!--user change the thumb's location by touching thumb/touching track,true for touching track to seek. false for touching thumb; default true-->
<!--        //indicator-->
<!--        //track-->
        <attr format="dimension|reference" name="tsb_track_background_size"/><!-- set indicatorSeekBar's track background bar size, default 2dp-->
        <attr format="color|reference" name="tsb_track_background_color"/><!-- set indicatorSeekBar's track background bar color, default #D7D7D7-->
        <attr format="dimension|reference" name="tsb_track_progress_size"/><!-- set indicatorSeekBar's track progress bar size, default 2dp-->
        <attr format="color|reference" name="tsb_track_progress_color"/><!-- set indicatorSeekBar's track progress bar color, default #FF4081-->
        <attr format="boolean" name="tsb_track_rounded_corners"/><!-- indicator is visible when SeekBar visible, gone when SeekBar gone-->
<!--        //thumb-->
        <attr format="color|reference" name="tsb_thumb_color"/><!--set thumb's color, default #FF4081-->
        <attr format="dimension|reference" name="tsb_thumb_size"/><!--set thumb's size, default 14dp, thumb size will be limited in 30dp-->
        <attr format="reference" name="tsb_thumb_drawable"/><!--set custom thumb's drawable you want,thumb size will be limited in 30dp, if drawable less than 30dp ,will show in raw size -->
        <attr format="boolean" name="tsb_thumb_adjust_auto"/><!--set the thumb move to the closed tick after touched up, default true-->
<!--        //thumb text-->
        <attr format="color|reference" name="tsb_thumb_text_color"/><!--set thumb's color, default #FF4081-->
        <attr name="tsb_show_thumb_text"><!--select the tick shape type, default rectangle/1-->
            <enum name="none" value="0"/>
            <enum name="below" value="1"/>
            <enum name="above" value="2"/>
        </attr>
<!--        //tickMarks-->
        <attr format="color|reference" name="tsb_tick_marks_color"/><!--set tick's color, default #FF4081-->
        <attr format="dimension|reference" name="tsb_tick_marks_size"/><!--set the tick width, default 10dp,custom drawable will be limited in 30dp, if less than 30dp ,will show in raw size-->
        <attr format="reference" name="tsb_tick_marks_drawable"/><!--set custom tick's drawable you want, custom drawable will be limited in 30dp, if less than 30dp ,will show in raw size-->
        <attr format="boolean" name="tsb_tick_marks_ends_hide"/><!--hide 2 ticks on the seekBar's both ends, default false-->
        <attr format="boolean" name="tsb_tick_marks_swept_hide"/><!--hide the ticks on the seekBar's thumb left, default false-->
        <attr name="tsb_show_tick_marks_type"><!--select the tick shape type, default rectangle/1-->
            <enum name="none" value="0"/>
            <enum name="oval" value="1"/>
            <enum name="square" value="2"/>
            <enum name="divider" value="3"/> <!--show tickMarks shape as vertical line , line'size is 1 dp.-->
            <enum name="custom" value="4"/>
        </attr>
<!--        //tickTexts-->
        <attr name="tsb_show_tick_texts"><!--select the tick shape type, default rectangle/1-->
            <enum name="none" value="0"/>
            <enum name="below" value="1"/>
            <enum name="above" value="2"/>
        </attr>
        <attr format="reference|color" name="tsb_tick_texts_color"/>
        <attr format="dimension|reference" name="tsb_tick_texts_size"/><!--set the text size of tick below text, default 13sp-->
        <attr format="reference" name="tsb_tick_texts_array"/><!--set the texts below tick to replace default progress text, default string of progress, work on seekBar type :DISCRETE_TICKS_TEXTS-->
        <attr name="tsb_tick_texts_typeface"><!--select the text typeface, default normal-->
            <enum name="normal" value="0"/>
            <enum name="monospace" value="1"/>
            <enum name="sans" value="2"/>
            <enum name="serif" value="3"/>
        </attr>
    </declare-styleable>
</resources>